case "/bookserver/createbook"_hash:
    {
        spdlog::info("createbook");
        JbBook reqtmp;
        try {
            auto reqtmp = request.get<JbBook>();
            CreateBook(db, &reqtmp);
        } catch (std::exception& e) {
            spdlog::info("{}", e.what());
        }
        break;
    }

